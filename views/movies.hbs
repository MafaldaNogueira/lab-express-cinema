<h1> Ironhack cinema</h1>
<p>Click on the movie to check the showtimes!</p> 




<div class="container">
  <div class="row">
    {{#each movies}}
    <div class="card" style="width: 15rem;">
      <img src={{image}} class="card-img-top" alt="card-image">
      <div class="card-body">
        <h5 class="card-title">{{title}}</h5>
        <a class="btn" href="/movies/{{_id}}">See more</a>
      </div>
    </div>
    {{/each}}
  </div>
</div>

